<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.github.twitchcaptain.deluge-unpacker</string>
        <key>ProgramArguments</key>
        <array>
            <string>/usr/local/bin/deluge-unpacker</string>
            <string>-c</string>
            <string>/usr/local/etc/deluge-unpacker/up.conf</string>
        </array>
        <key>RunAtLoad</key>
        <true/>
        <key>KeepAlive</key>
        <true/>
        <key>StandardErrorPath</key>
        <string>/usr/local/var/log/deluge-unpacker.log</string>
        <key>StandardOutPath</key>
        <string>/usr/local/var/log/deluge-unpacker.log</string>
    </dict>
</plist>
